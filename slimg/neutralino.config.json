{
    "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
    "version": "1.0.0",
    "defaultMode": "window",
    "port": 0,
    "documentRoot": "/resources/",
    "url": "/",
    "enableServer": true,
    "enableNativeAPI": true,
    "tokenSecurity": "one-time",
    "logging": {
        "enabled": true,
        "writeToLogFile": true
    },
    "nativeAllowList": [
        "app.*",
        "os.*",
        "filesystem.*",
        "debug.log",
        "window.*"
    ],
    "applicationId": "com.example.imagebrowser",
    "modes": {
        "window": {
            "title": "Image Browser",
            "width": 800,
            "height": 600,
            "minWidth": 400,
            "minHeight": 300,
            "resizable": true,
            "fullscreen": false,
            "maximize": false,
            "borderless": false
        }
    },
    "cli": {
        "binaryName": "neutralino",
        "resourcesPath": "/resources/",
        "extensionsPath": "/extensions/",
        "clientLibrary": "/resources/js/neutralino.js"
    }
}